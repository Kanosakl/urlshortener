    "url": "https://github.com/Kanosakl/urlshortener/issues"
  },
  "homepage": "https://github.com/Kanosakl/urlshortener#readme",
  "dependencies": {
    "body-parser": "^1.19.0",
    "bootstrap": "^4.4.1",
    "dotenv-safe": "^8.2.0",
    "express": "^4.17.1",
    "is-valid-http-url": "^1.0.3",
    "node-fetch": "^2.6.0",
    "react": "^16.12.0",
    "react-bootstrap": "^1.0.0-beta.16",
    "react-copy-to-clipboard": "^5.0.2",
    "react-dom": "^16.12.0"
