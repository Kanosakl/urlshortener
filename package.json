{
  "name": "urlshortener",
  "version": "1.0.0",
  "description": "url shortener homework",
  "main": "server.js",
  "scripts": {
    "watch": "npm run clean && parcel watch ./src/index.html",
    "debug": "node ./server.js --inspect-brk",
